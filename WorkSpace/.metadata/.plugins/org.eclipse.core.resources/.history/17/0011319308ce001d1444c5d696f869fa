package pack08_casting;

public class Ex02_Casting {
	public static void main(String[] args) {
		// 문자열 = "12" + 34 = 답은? 1234
		// String + int 답DataType은? String
		// Why? 문자열 + 어떤값 = 문자열 규칙
		// KYG(문자열) + 10 = 
		// 문자열 결합의 법칙 ↑
		// "1" == 1 ? 다르다
		// "1" (String) 다르다 1 (숫자)
		
		String kyg = "12" + 34;
		System.out.println(kyg);
		// 어떤 값을 String으로 바꾸는 것은 엄청나게 간단하다.
		String sum2 = ""+1234+"";
		System.out.println(sum2);
		
	}

}
