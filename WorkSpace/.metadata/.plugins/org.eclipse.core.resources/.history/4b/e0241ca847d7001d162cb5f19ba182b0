package pack02_array;

public class Ex02_Array {
	public static void main(String[] args) {
		// 1. 크기가 10의 크기를 가진 int형 배열을 만들고 값을 10~100까지 할당하고 출력해보기.
		// 출력 결과 : 배열이름[인덱스] : 10 ... 100
		int[] i = new int[10];
		i[0] = 10;
		i[1] = 20;
		i[2] = 30;
		i[3] = 40;
		i[4] = 50;
		i[5] = 60;
		i[6] = 70;
		i[7] = 80;
		i[8] = 90;
		i[9] = 100;
		for (int j = 0; j<10; j++) {
			System.out.println(i[j]);
		}
		
		
		
	}

}
