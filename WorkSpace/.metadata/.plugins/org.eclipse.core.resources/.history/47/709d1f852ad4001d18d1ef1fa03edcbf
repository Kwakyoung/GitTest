package pack06_game;

import java.util.Scanner;

public class Ex02_Grade {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		while(true){
			System.out.println("국어점수를 입력하세요.");
			int i = Integer.parseInt(sc.nextLine());
			if(i<=100) {
				break;
				}
			
			
		}
	}
}
